{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true,
    "**/.turbo": true,
    "**/dist": true,
    "**/pgdata": true,
    "**/redisdata": true,
  },
  "i18n-ally.localesPaths": "./apps/front/src/i18n/",
  "i18n-ally.enabledFrameworks": [
    "react"
  ],
  "i18n-ally.sortKeys": true,
  "i18n-ally.keepFulfilled": true,
  "i18n-ally.keystyle": "nested",
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "classNameBase",
    "classNameVariants",
    "ngClass",
    "class:list"
  ]
}